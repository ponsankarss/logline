<html>
<head>
    <link href="/static/css/bootstrap.css" rel="stylesheet"/>
    <link href="/static/css/bootstrap-responsive.css" rel="stylesheet"/>
    <link href="/static/css/prettify.css" rel="stylesheet"/>
    <link href="/static/css/jquery-ui-1.9.2.custom.css" rel="stylesheet"/>
    <link href="/static/css/jquery-ui.css" rel="stylesheet"/>
    <link href="/static/css/datepicker.css" rel="stylesheet"/>
    <link href="/static/css/logline.css" rel="stylesheet"/>

    <script src="/static/js/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="/static/js/jquery.highlight.js" type="text/javascript"></script>
    <script src="/static/js/html5.js" type="text/javascript"></script>
    <script src="/static/js/jquery-ui-1.9.2.custom.js" type="text/javascript"></script>
    <script src="/static/js/logline.js" type="text/javascript"></script>
    <title>Logline</title>
</head>
<body>
<div class="container-fluid">
    <div class="row-fluid mainbox">
        <fieldset>
            <h2>Blue2 Support</h2>

            <div id="form_table">
                <form id="log_form" class="form-horizontal" action="">
                    <div class="control-group">
                        <label for="folder" class="control-label">Logs Folder</label>

                        <div class="controls">
                            <input id="folder" type="text" class="required"/>
                        </div>
                    </div>
                    <div class="control-group">
                        <label for="keys" class="control-label">Keys</label>

                        <div class="controls">
                            <input id="keys" type="text" class="required"/>
                        </div>
                    </div>
                    <div id="buttons" class="form-actions">
                        <input type="button" id="analyse" value="Analyse"/>
                    </div>
                </form>
            </div>
        </fieldset>
        <div id="results"></div>
        <div id="loading-div-background">
            <div id="loading-div" class="ui-corner-all">
                <img src="http://www.jakobfolin.com/images/loading.gif" alt="Processing.."/>
                <h2 style="color:gray;font-weight:normal;">Please wait....</h2>
            </div>
        </div>
    </div>
</div>
</body>
</html>