<div>
    <span class="query-name">$recordSet.getName()</span>
    <span class="query-response">
        <table>
            <thead>
                    <tr>
                            #foreach($col in $recordSet.getColumns())
                                   <td>$col</td>
                            #end
                    </tr>
            </thead>
            <tbody>
                       #foreach($tuple in $recordSet.getTuples())
                                <tr>
                                    #foreach($item in $tuple.getItems())
                                    <td>$item</td>
                                     #end
                                 </tr>
                       #end
            </tbody>
        </table>
    </span>
</div>
